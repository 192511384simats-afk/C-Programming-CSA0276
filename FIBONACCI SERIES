#include <stdio.h>
int main()
{
  int n,first=0,second=1,fib,i;
  scanf("%d",&n);
  printf("FIBONACCI SERIES OF %d\n",n);
  printf("FIBONACCI SERIES IS ");
  if(n>=1)
  {
    printf("%d ",first);
  }
  for(i=2;i<=n;i++)
  {
    printf("%d ",second);
    fib=first+second;
    first=second;
    second=fib;
  }
  printf("\n");
  return 0;
}
