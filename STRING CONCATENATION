# include <stdio.h>
int main()
{
    char str1[50] = "I Am ";
    char str2[50] ="Iron Man";
    int i=0,j=0;
    while (str1[i] != '\0')
    {
        i++;
    }
    while (str2[j] != '\0')
    {
        str1[i] = str2[j];
        i++;
        j++;
    }
    
    printf("THE CONCATENATED STRING : %s\n",str1);
    return 0;
}
