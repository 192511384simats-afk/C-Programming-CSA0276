# include <stdio.h>
int main ()
{
    char str[50] = "why so serious";
    int i=0,j;
    char temp;
    while(str[i] != '\0')
    {
        i++;
        j=i-1;
    }
    for(i=0;i<j;i++,j--)
    {
        temp = str[j];
        str[j] = str[i];
        str[i] = temp;
    }
    printf("REVERSED STRING : %s\n",str);
    return 0;
}
